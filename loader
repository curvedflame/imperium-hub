local SCRIPT_MAP = {
    [21532277] = "https://raw.githubusercontent.com/curvedflame/imperium-hub/main/notoriety",
    [7006496598] = "https://raw.githubusercontent.com/curvedflame/imperium-hub/main/bleeding%20blades",
}

local _cache = {}
local url = SCRIPT_MAP[game.PlaceId]

if url then
    local source
    if _cache[url] then
        source = _cache[url]
    else
        source = game:HttpGet(url, true)
        _cache[url] = source
    end
    local func = loadstring(source)
    if func then
        func()
    else
        warn("[Imperium Hub] loadstring failed for URL:", url)
    end
else
    warn("[Imperium Hub] This place (" .. game.PlaceId .. ") isn't whitelisted.")
end
